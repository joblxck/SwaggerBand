{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "Track API",
    "description" : "API of Tracks",
    "version" : "1.0"
  },
  "servers" : [ {
    "url" : "https://virtserver.swaggerhub.com/JonasT3000/Band/1",
